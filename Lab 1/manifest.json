{
    "name": "Background Colour Changer",
    "version": "1.0",
    "description": "Changes the search engine's background colour to a random colour when clicked",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "Lab1HTML.html",
        "default_icon": {
            "16" : "img/16x.png",
            "32" : "img/32x.png",
            "48" : "img/48x.png"
        }
    },
    "manifest_version": 2
}